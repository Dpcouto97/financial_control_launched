<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    {% load static %}
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="{% static 'css/style.css' %}">  <!-- CSS for NavBar design mainly -->
    <link rel="stylesheet" href="{% static 'css/bootstrap.min.css' %}"> <!-- BootStrap CSS 1-->
    <link rel="stylesheet" href="{% static 'css/jquery.dataTables.min.css' %}"><!-- Tables CSS -->
    <link rel="stylesheet" href="{% static 'css/buttons.dataTables.min.css' %}"><!-- Tables CSS -->
    <link rel="stylesheet" href="{% static 'css/dataTables.bootstrap4.min.css' %}"> <!-- DataTable Bootstrap CSS -->
    <link rel="stylesheet" href="{% static 'css/jquery-ui.css' %}"><!-- DataPicker CSS -->

    <!-- Needed Scripts  -->
    <script src="{% static 'js/jquery-3.5.1.js' %}"></script> <!-- Jquery 3.5.1 -->
    <script src="{% static 'js/jquery.dataTables.min.js' %}"></script><!-- DataTable Script -->
    <script src="{% static 'js/dataTables.buttons.min.js' %}"></script><!-- DataTabel Export Buttons-->
    <script src="{% static 'js/buttons.html5.min.js' %}"></script> <!-- DataTabel Export Buttons Appear-->
    <script src="{% static 'js/pdfmake.min.js' %}"></script><!-- DataTabel Export PDF -->
    <script src="{% static 'js/vfs_fonts.js' %}"></script><!-- DataTabel Export PDF needed to Pdf export work -->
    <script src="{% static 'js/jquery-ui.js' %}"></script><!-- DataPicker Script -->
    <script src="{% static 'js/chart.min.js' %}"></script> <!-- Charts.js -->

    <title>Usecase Management Platform</title>
  </head>
  <body>
    <!-- NavBar/Menu -->
    <div class="bg-light sticky-top">
        {% include 'navbar.html'  %}
    </div>

    <!-- Page Body/Content -->
    <div class="container pt-4">
         {% block content %}
         Replace me Block
        {% endblock %}
    </div>
  </body>

  <!-- Popper.js, then Bootstrap JS -->
  <script src="{% static 'js/popper.min.js' %}"></script>
  <script src="{% static 'js/bootstrap.min.js' %}"></script>
</html>

